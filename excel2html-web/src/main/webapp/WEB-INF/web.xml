<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

<!-- [START standard_mappings] -->
    <servlet>
        <servlet-name>excel2html</servlet-name>
        <servlet-class>kshiroma0622.excel2html.web.servlet.Excel2HtmlServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>excel2html</servlet-name>
        <url-pattern>/excel2html</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>excellist.jsp</welcome-file>
    </welcome-file-list>
<!-- [END standard_mappings] -->

